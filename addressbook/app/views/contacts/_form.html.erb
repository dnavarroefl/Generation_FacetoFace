<%= form_for @contact do |f| %> 
	 <%= f.label :name %> 
	 <%= f.text_field :name %> 
	 <br>
	 <%= f.label :address %>
	 <%= f.text_field :address %> 
	 <br> 
	 <%= f.label :phonenumber %> 
	 <%= f.text_field :phonenumber %> 
	 <br>
	 <%= f.label :email %> 
	 <%= f.text_field :email %> 
 
	 <%= f.submit 'Save' %> 
<% end %>
	